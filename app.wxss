/**app.wxss**/
@import "fonts/fontAwesome.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.bg {
  width: 100%;
  height: 100%;
  z-index: -1;
  position: fixed;
}
.logo {
  width: 200rpx;
  height: 200rpx;
  margin: 40% 0;
  border-radius: 50%;
  background-color: rgba(255, 255, 255, 1);
}
.name {
  margin: 70% 0;
  z-index: 100;
  position: fixed;
  color: rgba(255, 255, 255, 1);
}

